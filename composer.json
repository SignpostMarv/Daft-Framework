{
    "name": "signpostmarv/daft-framework",
    "description": "Daft Framework",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "SignpostMarv",
            "email": "me@signpostmarv.name"
        }
    ],
    "autoload": {
        "psr-4": {
            "SignpostMarv\\DaftFramework\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FastRoute\\": "./vendor/nikic/fast-route/test/",
            "SignpostMarv\\DaftInterfaceCollector\\Tests\\": "./vendor/signpostmarv/daft-interface-collector/Tests/",
            "SignpostMarv\\DaftFramework\\Tests\\": "./Tests/",
            "SignpostMarv\\DaftRouter\\Tests\\": "./vendor/signpostmarv/daft-router/Tests/"
        }
    },
    "config": {
        "classmap-authoritative": true,
        "sort-packages": true
    },
    "scripts": {
        "tests": [
            "phpunit -vvvv --coverage-clover=phpunit.clover",
            "psalm --find-dead-code --shepherd",
            "phpcpd src Tests",
            "phpmnd ./ --exclude=./phpunit/ --exclude=./vendor/ --non-zero-exit-on-violation --hint",
            "composer-require-checker check ./composer.json",
            "php-cs-fixer fix --verbose --allow-risky=yes --dry-run"
        ]
    },
    "require": {
        "php": "^7.3",
        "ext-mbstring": "*",
        "paragonie/easydb": "^2.9",
        "signpostmarv/daft-interface-collector": "dev-master",
        "signpostmarv/daft-router": "dev-typed-args",
        "symfony/console": "^4.3",
        "symfony/event-dispatcher": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/http-kernel": "^4.3"
    },
    "require-dev": {
        "infection/infection": "^0.13.1",
        "maglnet/composer-require-checker": "^2.0",
        "phpunit/phpunit": "^7.5",
        "povils/phpmnd": "^2.1",
        "psalm/plugin-phpunit": "^0.6.0",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^4.1",
        "signpostmarv/php-cs-fixer-config": "^0.10.0",
        "vimeo/psalm": "^3.4"
    }
}
